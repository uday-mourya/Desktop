<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Mobile" table="Mobile">
        <id name="mobileId" type="int">
            <column name="mobile_id" />
            <generator class="assigned" />
        </id>
        <property name="brand" column="brand" not-null="true" />
        <property name="model" column="model" not-null="true" />
        <property name="storageCapacity" column="storage_capacity" not-null="true" />
        <property name="color" column="color" not-null="true" />
        <property name="price" column="price" not-null="true" />
        <property name="releaseDate" column="release_date" not-null="true" />
    </class>
</hibernate-mapping>


